{
    "name": "darthsoup/laravel-theme",
    "description": "Laravel Theme System",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Krummnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/contracts": "^6|^7|^8",
        "illuminate/support": "^6|^7|^8",
        "illuminate/view": "^6|^7|^8"
    },
    "require-dev": {
        "mockery/mockery": "~1.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "5.x"
    },
    "autoload": {
        "psr-4": {
            "DarthSoup\\Theme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarthSoup\\Tests\\Theme\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "DarthSoup\\Theme\\ThemeServiceProvider"
            ],
            "aliases": {
                "Theme": "DarthSoup\\Theme\\Facades\\Theme"
            }
        }
    }
}
